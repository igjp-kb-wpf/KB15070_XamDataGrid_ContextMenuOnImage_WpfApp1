<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\BlueStar.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\BlueStar.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Infragistics.WPF.DataGrids" Version="24.2.53" />
    <PackageReference Include="Infragistics.WPF.Menus" Version="24.2.53" />
  </ItemGroup>

</Project>
